;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	dns-vm.cf.plumgrid.com. admin.cf.plumgrid.com. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
; name servers - NS records
        IN      NS      dns-vm.cf.plumgrid.com.

;
; PTR Records
116      IN      PTR     dns-vm.cf.plumgrid.com.    ; 10.10.0.116
90      IN      PTR     api.cf.plumgrid.com.  ; 192.168.14.90


